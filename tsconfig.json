{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [ "es6" ],
    "sourceMap": true,
    "declaration": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "incremental": true,
    "allowJs": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "include": [
    "./src",
    "./src/test/**/*.js",
    "./src/**/*.json"
  ],
  "exclude": [
    "node_modules" , "./dist" , "./webApp"
  ]
}
