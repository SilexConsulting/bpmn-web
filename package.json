  "private": true,
  "scripts": {
    "setup": "ts-node src/scripts/setup.ts",
    "start": "ts-node src/app",
    "cli": "ts-node src/scripts/cli.ts",
    "build": "tsc --build",
    "compileRun": "npm run build && node dist/app.js",
    "dev": "nodemon -e ts --watch ./ --watch ../bpmn-server --exec \"npm run compileRun\"",
    "clean": "rimraf dist",
    "start:watch": "tsc-watch --project . --onSuccess \"nodemon ./src/app.ts\"",
    "test": "npm run build && mocha -exit --config=src/test/.mocharc.json dist/test/feature/"
  },
